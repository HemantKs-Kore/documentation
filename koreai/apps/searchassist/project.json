      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "searchassist:build:production"
        },
        "development": {
          "browserTarget": "searchassist:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "searchassist:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/searchassist/**/*.ts",
          "apps/searchassist/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/searchassist"],
      "options": {
        "jestConfig": "apps/searchassist/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
