{
  "name": "findly-builder",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "ng serve searchassist -o",
    "serve:app": "ng build searchassist --configuration=app && live-server dist/apps/searchassist",
    "serve:qa": "ng build searchassist --configuration=qa && npm run liveserver",
    "serve:prod": "run-s build liveserver",
    "liveserver": "cd dist/apps/searchassist && live-server",
    "build": "ng build searchassist --base-href /home/",
    "ng": "nx",
    "lint": "ng lint searchassist",
    "lintfix": "nx lint searchassist --fix",
    "test": "ng test searchassist",
    "e2e": "ng e2e searchassist-e2e --watch",
    "postinstall": "node ./decorate-angular-cli.js",
    "generate": "ts-node ./server/generate.js > ./server/database.json",
    "server": "ts-node ./server/server.ts",
    "sme-build": "ng build searchassist --source-map",
    "sme": "npm run sme-build && source-map-explorer dist/**/*.js --no-border-checks",
    "wba-build": "ng build searchassist --stats-json",
    "wba": "npm run wba-build && webpack-bundle-analyzer dist/apps/searchassist/stats.json",
    "sonar": "sonar-scanner",
    "compodoc": "compodoc -p apps/searchassist/tsconfig.doc.json --includes docs -d dist/documentation && npm run copyDocsAsset",
    "copyDocsAsset": "cpx \"docs/images/*\" dist/documentation/additional-documentation/images",
    "uncss": "node tools/scripts/uncss.js",
    "criticalCss": "node tools/scripts/csscoverage.js",
    "perf:report": "lighthouse --output-path=./dist/lighthouse-report.html https://searchassist-app.kore.ai/home/ --view --preset=desktop --port 51745",
    "deploy": "ngh --dir=dist --message=\"deploy\"",
    "audit": "npm audit --omit=dev",
    "buildjs": "node ./tools/build.js",
    "update-angular": "ng update @angular/cli @angular/core"
  },
  "private": true,
  "dependencies": {
