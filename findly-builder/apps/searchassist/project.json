{
  "name": "searchassist",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/searchassist/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/searchassist",
        "index": "apps/searchassist/src/index.html",
        "main": "apps/searchassist/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/searchassist/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "stylePreprocessorOptions": {
          "includePaths": ["scss", "apps/searchassist/src/scss"]
        },
        "assets": [
          "apps/searchassist/src/favicon.ico",
          "apps/searchassist/src/assets",
          {
            "glob": "**/*",
            "input": "assets/",
            "output": "assets/"
          },
          "apps/searchassist/src/manifest.webmanifest"
        ],
        "styles": [
          "apps/searchassist/src/scss/styles.scss",
          {
            "input": "scss/themes/dark.scss",
            "bundleName": "dark-theme",
            "inject": false
          },
          {
            "input": "apps/searchassist/src/scss/components/codemirror.scss",
            "bundleName": "codemirror.min",
            "inject": false
          },
          {
            "input": "apps/searchassist/src/scss/components/searchsdk.scss",
            "bundleName": "searchsdk.min",
            "inject": false
          },
          {
            "input": "apps/searchassist/src/scss/components/vendor.scss",
            "bundleName": "vendor.min",
            "inject": false
          }
        ],
        "scripts": [
          {
            "input": "apps/searchassist/src/assets/js/scripts.min.js",
            "bundleName": "scripts.min",
            "inject": false
          }
        ],
        "serviceWorker": true,
        "ngswConfigPath": "apps/searchassist/ngsw-config.json"
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "2mb",
              "maximumError": "5mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "106kb",
              "maximumError": "110kb"
            },
            {
              "type": "bundle",
              "name": "vendor",
              "baseline": "750kb",
              "warning": "1.5mb",
              "error": "2mb"
            },
            {
              "type": "bundle",
              "name": "main",
              "baseline": "500kb",
              "warning": "750kb",
              "error": "1mb"
            }
          ],
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.prod.ts"
            }
          ],
          "outputHashing": "all",
          "vendorChunk": true,
          "namedChunks": true
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "prod": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.prod.ts"
            }
          ]
        },
        "clouddev": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.clouddev.ts"
            }
          ]
        },
        "app": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.app.ts"
            }
          ],
          "sourceMap": true,
          "namedChunks": true
        },
        "qa": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.qa.ts"
            }
          ]
        },
        "uat": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.uat.ts"
            }
          ]
        },
        "onprem": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.onprem.ts"
            }
          ]
        },
        "qa1": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.qa1.ts"
            }
          ]
        },
        "pilot": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.pilot.ts"
            }
          ]
        },
        "loadtest": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.loadtest.ts"
            }
          ]
        },
        "qabots": {
          "fileReplacements": [
            {
              "replace": "apps/searchassist/src/environments/environment.ts",
              "with": "apps/searchassist/src/environments/environment.qabots.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "searchassist:build:production"
        },
        "development": {
          "browserTarget": "searchassist:build:development"
        },
        "app": {
          "browserTarget": "searchassist:build:app"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "searchassist:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "apps/searchassist/src/**/*.ts",
          "apps/searchassist/src/**/*.html"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/searchassist"],
      "options": {
        "jestConfig": "apps/searchassist/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
